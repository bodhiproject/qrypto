{
  "name": "Qrypto",
  "version": "1.0.0",
  "scripts": {
    "start": "yarn clean && mkdir dist && ./scripts/create-empty-thunk.sh && webpack --watch --devtool eval-cheap-module-source-map & yarn css-types -w",
    "build": "yarn clean && webpack && ./scripts/create-empty-thunk.sh",
    "clean": "rm -rf dist",
    "css-types": "tcm src",
    "postinstall": "yarn css-types",
    "linter": "tslint --project .",
    "linter-fix": "tslint --fix --project .",
    "precommit": "lint-staged",
    "prepush": "npm run linter-fix"
  },
  "lint-staged": {
    "*.{ts,tsx,js}": [
      "npm run linter-fix",
      "git add"
    ]
  },
  "devDependencies": {
    "@ceoimon/typed-css-modules": "^0.0.1",
    "@material-ui/core": "^1.2.2",
    "@types/chrome": "^0.0.63",
    "@types/classnames": "^2.2.4",
    "@types/prop-types": "^15.5.3",
    "@types/react": "^16.0.41",
    "@types/react-dom": "^16.0.4",
    "@types/react-router-dom": "^4.2.7",
    "autoprefixer": "^8.2.0",
    "copy-webpack-plugin": "^4.5.1",
    "css-loader": "^0.28.11",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.11",
    "husky": "^0.14.3",
    "lint-staged": "^7.0.0",
    "node-sass": "^4.9.0",
    "postcss-flexbugs-fixes": "^3.3.0",
    "postcss-loader": "^2.1.3",
    "postcss-modules": "^1.1.0",
    "react-router-dom": "^4.3.1",
    "sass-loader": "^7.0.3",
    "style-loader": "^0.20.3",
    "ts-loader": "3.5.0",
    "tslint": "^5.9.1",
    "tslint-loader": "^3.6.0",
    "typescript": "^2.7.2",
    "url-loader": "^1.0.1",
    "web-ext-types": "^2.0.1",
    "webpack": "3.11.0"
  },
  "dependencies": {
    "@material-ui/icons": "^1.1.0",
    "@types/lodash": "^4.14.110",
    "axios": "^0.18.0",
    "chrome-call": "^3.0.0",
    "classnames": "^2.2.6",
    "mobx": "^5.0.3",
    "mobx-react": "^5.2.3",
    "qrcode.react": "^0.8.0",
    "qtumjs-wallet": "^0.0.6",
    "react": "^16.2.0",
    "react-dom": "^16.2.0"
  },
  "private": true
}
